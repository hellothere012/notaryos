{
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "src/app/r/[hash]/page.tsx": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.agenttownsquare.com"
  },
  "redirects": [
    {
      "source": "/index.html",
      "destination": "/",
      "permanent": true
    }
  ]
}
